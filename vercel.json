{
    "version": "2",
    "builds":[
        {
            "src":"dist/src/index.js",
            "use": "@vercel/node",
            "config": { 
                "includefiles": "dist/src/**"
             }
        }
    ],
    "routes": [
        { "src": "/.*", "dest": "dist/src/index.js" }
    ]
}